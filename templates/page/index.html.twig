@keyframes lazymove {
  0% {transform: translate(0px,-20px);}
  100% {transform: translate(0px,20px);}
}

@keyframes lazymove2 {
  0% {transform: rotate(6deg) scale(1);}
  100% {transform:rotate(0deg) scale(0.9);}
}

@keyframes lazymove3 {
  0% {transform: translate(0px,-5px) scale(1);}
  100% {transform: translate(0px,5px) scale(0.8);}
}

@keyframes lazymove4 {
  0% {transform: rotate(-3deg) scale(1);}
  100% {transform:rotate(0deg) scale(0.9);}
}

@keyframes shadowmove {
  0% {opacity:0.3; transform:scaleX(1.2) perspective(900px) rotatex(80deg);}
  100% {opacity:0.8; transform:scaleX(0.7) perspective(900px) rotatex(80deg);}
}



